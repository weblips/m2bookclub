    /**
     * constructor
     *
     * @return void
     */
    protected function _construct()
    {
        $this->_controller = 'adminhtml_{{entity}}';
        $this->_blockGroup = '{{Namespace}}_{{Module}}';
        $this->_headerText = __('{{EntitiesLabel}}');
        $this->_addButtonLabel = __('Create New {{EntityLabel}}');
        parent::_construct();
    }
